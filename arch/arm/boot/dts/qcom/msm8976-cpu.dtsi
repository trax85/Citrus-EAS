 			            167180 //	1113600 kHZ
			            261430 //	1248000 kHZ
			            317200 //	1382400 kHZ
			            352870 //	1612800 kHZ
			            374360 //	1747200 kHZ
			            443880 >;// 1804800 kHZ
			L2_1: l2-cache {
			      compatible = "arm,arch-cache";
			      cache-level = <2>;

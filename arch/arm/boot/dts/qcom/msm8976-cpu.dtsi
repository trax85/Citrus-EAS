                        // frequency table.
 			current = < 18250 // 	 400000 kHZ
			            24330 // 	 691200 kHZ
			            26920 // 	 806400 kHZ
			            29120 //	 960000 KHz
			            34600 //	1017600 kHz
			            38150 //	1190400 kHZ
			            57940 //	1382400 kHZ
			            81740 >;//	1401600 kHZ
				    //112000>;//	1536000 kHZ
			qcom,sleep-status = <&cpu1_slp_sts>;
		};

		CPU2: cpu@2 {

			next-level-cache = <&L2_1>;
                        sched-energy-costs = <&CPU_COST_a72 &CLUSTER_COST_A72>;
 			// The currents(uA) correspond to the frequencies in the
                        // frequency table.
 			current = < 67740 // 	 400000 kHZ
			            82960 // 	 883200 kHZ
			            133160 // 	 998400 kHZ
			            150160 //	1056000 kHZ
 			            167180 //	1113600 kHZ

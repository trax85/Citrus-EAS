/ {

	psci {
		compatible = "arm,psci-1.0";
		method = "smc";
	};

	cpus {
		#address-cells = <1>;
		#size-cells = <0>;
		cpu-map {
			cluster0 {
				core0 {
					cpu = <&CPU0>;
				};
				core1 {
					cpu = <&CPU1>;
				};
				core2 {
					cpu = <&CPU2>;
				};
				core3 {
					cpu = <&CPU3>;
				};
			};

			cluster1 {
				core0 {
					cpu = <&CPU4>;
				};
				core1 {
					cpu = <&CPU5>;
				};
				core2 {
					cpu = <&CPU6>;
				};
				core3 {
					cpu = <&CPU7>;
				};
			};
		};

		CPU0: cpu@0 {
			device_type = "cpu";
			compatible = "arm,cortex-a53","arm,armv8";
			reg = <0x0>;
			clock-frequency = <1401600000>;
			enable-method = "psci";
			qcom,limits-info = <&mitigation_profile0>;
			efficiency = <1024>;
			qcom,acc = <&acc0>;
			next-level-cache = <&L2_0>;
                        sched-energy-costs = <&CPU_COST_A53 &CLUSTER_COST_A53>;
                        // The currents(uA) correspond to the frequencies in the
                        // frequency table.
 			current = < 18250 // 	 400000 kHZ
			            24330 // 	 691200 kHZ
			            26920 // 	 806400 kHZ
			            29120 //	 960000 KHz
			            34600 //	1017600 kHz
			            38150 //	1190400 kHZ
			            57940 //	1382400 kHZ
			            81740 >;//	1401600 kHZ
				    //112000>;//	1536000 kHZ
			qcom,sleep-status = <&cpu0_slp_sts>;
			L2_0: l2-cache {
			      compatible = "arm,arch-cache";
			      cache-level = <2>;
			      power-domain = <&l2ccc_0>;
			};
		};

		CPU1: cpu@1 {
			device_type = "cpu";
			compatible = "arm,cortex-a53","arm,armv8";
			reg = <0x1>;
